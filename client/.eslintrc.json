{
  "extends": "next/core-web-vitals",
  "plugins": [
    "simple-import-sort"
  ],
  "rules": {
    "indent": ["error", "tab", {
      "SwitchCase": 1
    }],
    "no-unused-vars": ["error", {
      "vars": "all",
      "args": "all",
      "ignoreRestSiblings": false
    }],
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  }
}